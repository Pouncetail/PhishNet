{
  "manifest_version": 3,
  "name": "PhishNet AI",
  "version": "1.0",
  "description": "This is a popup-based Chrome extension",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Link Checker"
  }
}
